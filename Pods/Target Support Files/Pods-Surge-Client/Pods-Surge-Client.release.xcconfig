FRAMEWORK_SEARCH_PATHS = "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/CRToast.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/INTULocationManager.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/JSONJoy.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/JSQMessagesViewController.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/JSQSystemSoundPlayer.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/SwiftHTTP.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "CRToast" -framework "INTULocationManager" -framework "JSONJoy" -framework "JSQMessagesViewController" -framework "JSQSystemSoundPlayer" -framework "SwiftHTTP"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Surge-Client
PODS_ROOT = ${SRCROOT}/Pods